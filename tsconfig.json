{
  "compilerOptions": {
    "target": "ES2017",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./src/private",
    "outDir": "./build",

  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "build"]
}